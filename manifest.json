{
    "manifest_version": 2,
    
    "name": "한위백 편집 도우미",
    "description": "한국어 위키백과의 문서 편집을 도와줍니다",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
		"tabs",
        "activeTab",
        "<all_urls>"
    ]
}
